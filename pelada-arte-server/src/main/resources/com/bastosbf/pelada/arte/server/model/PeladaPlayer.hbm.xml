<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.bastosbf.pelada.arte.server.model.PeladaPlayer" table="pelada_player" lazy="false">
		<composite-id>
			<key-many-to-one name="pelada" class="com.bastosbf.pelada.arte.server.model.Pelada"></key-many-to-one>
			<key-many-to-one name="player" class="com.bastosbf.pelada.arte.server.model.Player"></key-many-to-one>
		</composite-id>		
	</class>
</hibernate-mapping>
