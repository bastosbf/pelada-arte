<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.bastosbf.pelada.arte.server.model.Pelada" table="pelada" lazy="false">
		<id name="id" column="id">
			<generator class="increment"/>
		</id>
		<many-to-one name="owner" class="com.bastosbf.pelada.arte.server.model.Player" fetch="select" />
		<property name="name" />		
		<property name="day" />
		<property name="time"/>
	</class>
</hibernate-mapping>