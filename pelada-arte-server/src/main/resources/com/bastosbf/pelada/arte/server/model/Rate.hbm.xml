<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.bastosbf.pelada.arte.server.model.Rate" table="rate" lazy="false">
	<composite-id>
	 	<key-many-to-one name="pelada" class="com.bastosbf.pelada.arte.server.model.Pelada"/>
	 	<key-property name="date"/>
	 	<key-many-to-one name="rate_from" class="com.bastosbf.pelada.arte.server.model.Player" />
	 	<key-many-to-one name="rate_to" class="com.bastosbf.pelada.arte.server.model.Player"/>
	</composite-id>
	<property name="rate"/>		
	</class>
</hibernate-mapping>